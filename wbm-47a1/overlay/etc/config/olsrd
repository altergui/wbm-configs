
config olsrd
	option IpVersion '6'
	option LinkQualityAlgorithm 'etx_ffeth'

config LoadPlugin
	option library 'olsrd_arprefresh.so.0.1'

config LoadPlugin
	option library 'olsrd_jsoninfo.so.0.0'
	option accept '::1'
	option port '9090'

config LoadPlugin
	option library 'olsrd_txtinfo.so.0.1'
	option accept '::1'
	option port '2006'

config Interface
	option interface 'lan_olsr'
	option IPv6Multicast 'ff02::6D'
	option speed '5'
	option Mode 'ether'

config Interface
	option interface 'wbm1_olsr'
	option IPv6Multicast 'ff02::6D'
	option speed '5'
	option Mode 'mesh'

