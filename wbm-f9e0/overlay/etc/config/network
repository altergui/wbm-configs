	option ipaddr '10.5.249.224/16'
	option ip6addr 'fdba:5:f9e0::1/64'
	option proto 'batadv'
	option mesh 'bat1'
	option mtu '1528'

config interface 'bat0'
	option ifname 'bat0'
	option proto 'static'
	option mtu '1500'
	option macaddr '02:ba:f9:e0:00:01'
	option ip6addr 'fdba:3:f9e0::1/64'
	option ipaddr '10.3.249.224'
	option netmask '16'

config interface 'bat1'
	option ifname 'bat1'
	option proto 'dhcp'

config interface 'bat1_v6'
	option ifname '@bat1'
	option proto 'dhcpv6'

config interface 'wbm1_base'
